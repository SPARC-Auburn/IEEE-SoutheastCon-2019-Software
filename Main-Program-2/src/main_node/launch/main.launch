<launch>


	<node pkg="main_node" name="main" type="main" required="true" output="screen"/>
	<node pkg="opencv_node" name="vision" type="vision_talker" required="true" output="screen"/>
	<arg name="razor_config_file" default="$(find razor_imu_9dof)/config/my_razor.yaml"/>
  	<node pkg="razor_imu_9dof" type="imu_node.py" name="imu_node" output="screen">
    		<rosparam file="$(arg razor_config_file)" command="load"/>
		<remap from="imu" to="sensor_msgs/Imu"/>
 	 </node>
	<node pkg="rosserial_arduino" name="serialclient" type="serial_node.py" required="true" output="screen">
		<param name="port" value="/dev/ttyUSB0"/>
	</node>



</launch>
